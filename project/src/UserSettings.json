{
	"static": {
		"patternsFile": "project/patterns/Patterns.java"
	},
	"output": {
		"path": "out/",
		"format": "json"
	},
	"global": {
		"numberOfThreads": 32,
		"parseComments": false
	},
	"dynamic": {
		"innerLoops": true,
		"possibleTernary": true,
		"countMethods": true,
		"countMethodsPrivate": true,
		"countMethodsPublic": true,
		"countMethodsProtected": true,
		"linesOfCodeClass": true,
		"linesOfCodeMethod": true,
		"countComments": true,
		"countCommentsInline": true,
		"countCommentsBlock": true,
		"countCommentsJavadoc": true,
		"countClassFields": true,
		"countClassFieldsPublic": true,
		"countClassFieldsProtected": true
	}
}