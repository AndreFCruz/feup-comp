{
	"static": {
		"patternsFile": "project/patterns/Patterns.java"
	},
	"output": {
		"path": "out/",
		"format": "json"
	},
	"global": {
		"numberOfThreads": 32,
		"parseComments": true
	},
	"dynamic": {
		"innerLoops": true,
		"possibleTernary": true,
		"countMethods": true,
		"countMethodsPrivate": true,
		"countMethodsPublic": true,
		"countMethodsProtected": true,
		"linesOfCodeClass": true,
		"linesOfCodeMethod": true,
		"countComments": true,
		"countCommentsInline": true
	}
}