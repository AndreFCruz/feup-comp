{
	"static": {
		"patternsFile": "project/patterns/Patterns.java-"
	},
	"output": {
		"path": "out/",
		"format": "json"
	},
	"global": {
		"numberOfThreads": 32,
		"parseComments": false,
		"operations": ["sum", "min", "max", "std", "avg"]
	},
	"dynamic": {
		"innerLoops": false,
		"possibleTernary": false,
		"ternary": false,
		"cyclomaticComplexity": false,
		"loopsFor": false,
		"loopsForeach": false,
		"loopsWhile": false,
		"classMethods": true,
		"classMethodsPrivate": false,
		"classMethodsPublic": false,
		"classMethodsProtected": false,
		"classMethodsStatic": false,
		"classMethodsAbstract": false,
		"linesOfCodeClass": false,
		"linesOfCodeMethod": false,
		"classComments": false,
		"classCommentsInline": false,
		"classCommentsBlock": false,
		"classCommentsJavadoc": false,
		"classFields": false,
		"classFieldsPublic": false,
		"classFieldsProtected": false,
		"classFieldsPrivate": false,
		"classFieldsStatic": false,
		"superClasses": false,
		"superClassesJava": false,
		"weightedMethodCountLoC": false,
		"weightedMethodCountCC": false,
		"weightedMethodCountNoM": false
	}
}